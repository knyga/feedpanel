<html>
<head>
    <meta charset="UTF-8">
    <title>Feed</title>
</head>
<body>
    <table>
        <% items.forEach(function(item){ %>
        <tr>
            <td><a target="_blank" href="<%= item.thumb %>" /><img width="50" src="<%= item.thumbnail %>" /></a></td>
            <td><a target="_blank"href="<%= item.link %>" /><%= item.title %></a></td>
            <td><%- item.description %></td>
            <td><%= item.pubDate %></td>
        </tr>
        <% }); %>
    </table>

</body>
</html>